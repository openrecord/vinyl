{
  "name": "vinyl",
  "version": "0.0.1",
  "description": "Frontend for openrecord platform.",
  "main": "TODO",
  "scripts": {
    "clean": "mkdir -p turntable/generated && touch turntable/generated/prisma.ts",
    "start": "docker-compose -f turntable/docker-compose.yml up -d && npm run start:web & npm run start:api",
    "start:web": "webpack-dev-server --mode development",
    "start:api": "netlify-lambda serve --config turntable/webpack.config.js turntable/functions",
    "serve:debug": "NODE_ENV=development ENDPOINT='http://localhost:4466' npx --node-arg=--inspect-brk netlify-lambda serve --config webpack.config.js turntable/functions",
    "build": "npm run build:api && npm run build:web",
    "build:web": "webpack -c webpack.config.js --mode production",
    "build:api": "npm run clean && npm run codegen && npm run compile:api",
    "deploy:test": "cd turntable; env-cmd ../env.test.env prisma deploy",
    "deploy:dev": "cd turntable; env-cmd ../env/.dev.env prisma deploy",
    "deploy:staging": "cd turntable; env-cmd ../env/.staging.env prisma deploy",
    "deploy:prod": "cd turntable; env-cmd ../env/.prod.env prisma deploy",
    "codegen": "cd turntable; get-graphql-schema $ENDPOINT > schema.graphql && gql codegen",
    "compile:api": "netlify-lambda build --config turntable/webpack.config.js turntable/functions",
    "test": "jest --runInBand",
    "test:ci": "jest --runInBand --ci || jest --runInBand --ci",
    "test:debug": "ndb jest --runInBand",
    "test:env-up": "npm run stop:docker && npm run test:fresh-docker && yarn start:web",
    "test:fresh-docker": "docker-compose -f docker-compose.test.yml rm -fv postgres prisma turntable && docker-compose -f docker-compose.test.yml up -d --build prisma && docker-compose -f docker-compose.test.yml up -d turntable",
    "stop:docker": "docker-compose -f docker-compose.test.yml stop && docker-compose -f turntable/docker-compose.yml stop",
    "config": "node bin/printConfig.js",
    "makemigrations": "cd turntable; tsc",
    "migrate": "npm run makemigrations && node turntable/built/migrations/index.js",
    "migrate:prod": "env-cmd env/.prod.env npm run migrate",
    "toml": "babel-node scripts/create_netlify_env.js"
  },
